{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "lib": ["ES2022"],
    "outDir": "dist",
    "rootDir": "src",
    "strict": false,
    "noImplicitAny": false,
    "noUncheckedIndexedAccess": false,
    "useUnknownInCatchVariables": false,
    "esModuleInterop": false,
    "skipLibCheck": true,
  },
  "include": ["src"]
}